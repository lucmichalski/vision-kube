<div class="row">
  <div class="col-lg-12">
    <h3>Backends</h3>

    <table class="table table-bordered">
      <thead>
        <tr>
          <th>ID</th>
          <th>Type</th>
        </tr>
      </thead>
      <tbody>
      {{#each backend in model}}
        <tr>
          <td>{{#link-to "backends.edit" backend.id}} {{backend.id}} {{/link-to}}</td>
          <td>{{backend.type}}</td>
        </tr>
      {{/each}}
      </tbody>
    </table>
  </div>
</div>